@model  Synzeal_Inventory.Entity.SZ_QuotationDetail
@{
    ViewBag.Title = "Company";
    Layout = null;
    string sectionName = ViewBag.sectionName;

}
<div class="col-md-12">
    <div class="box">
        <!-- form start -->
        <form role="form">
            <div class="box-body">
                 <div class="form-group">
                     <label for="exampleInputEmail1">GLP Remark</label>
                    <textarea class="form-control" id="GLPRemark" name="GLPRemark" rows="25" value="@Model.GLPRemark"></textarea>
                    <input type="hidden" id="GLPRemarkid" name="GLPRemarkid" value="@Model.Id" />
                </div>
                <div class="form-group" style="text-align: right">
                    <button type="button" onclick="Quote.addGLPRemark()" class="btn btn-primary">Save</button>
                </div>
            </div>
        </form>
    </div>
</div>

@if (!string.IsNullOrEmpty(Model.GLPRemark))
{
    <text>
        <script type="text/javascript">
            var test = $("#GLPRemark").attr('value');
            document.getElementById("GLPRemark").value = test;
        </script>
    </text>
}