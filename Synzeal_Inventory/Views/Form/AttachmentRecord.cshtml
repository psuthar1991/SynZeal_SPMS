<div class="col-md-12">
    <!-- general form elements -->
    <!-- /.box-header -->
    <div class="box box-primary">

        <!-- form start -->
        <form role="form">
            <div class="box-body">
                <div class="form-group">
                    <label for="exampleInputEmail1">Suggested Chemical Name</label>
                    <div>
                        @ViewBag.SuggChemName
                    </div>
                </div>
                <div class="form-group">
                    <label for="exampleInputEmail1">Download Attachment</label>
                    <div>
                        @if (!string.IsNullOrEmpty(ViewBag.Attachment))
                        {
                            string[] data = Convert.ToString(ViewBag.Attachment).Split(',');
                            foreach (var i in data)
                            {
                                <a href="/Content/Attachment/@i" target="_blank">@i</a> <br />
                            }
                        }
                    </div>
                </div>
            </div>
        </form>
    </div>
</div>


<div class="clearfix"></div>