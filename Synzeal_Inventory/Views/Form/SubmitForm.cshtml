@{
    ViewBag.Title = "SubmitForm";
    Layout = "~/Views/Shared/_Layout.cshtml";
    int quotationDetailsId = ViewBag.QuotationDetailsId;

    var formQuotationData = new SZ_QuoteDetailForm();
    if (ViewBag.FormId != null && ViewBag.FormId != 0)
    {
        formQuotationData = ViewBag.FormData as SZ_QuoteDetailForm;
    }
    string correctionstatus = Convert.ToString((int)Synzeal_Inventory.Models.EnumList.ApprovedStatus.QCCorrection);
    bool Isdispatch = Convert.ToBoolean(ViewBag.Isdispatch);
    bool isPrint = Convert.ToBoolean(ViewBag.IsPrint);
    var comments = ViewBag.QuoteDetailsComment as List<SZ_QuoteDetailsComment>;
    var islock = Convert.ToBoolean(ViewBag.islockForm);
    bool IsNewProductMaster = Convert.ToBoolean(ViewBag.NewProductMaster);
}
@model SZ_QuotationDetail
@using Synzeal_Inventory.Entity

<section class="content-header">
    <h1>
        Product Submission Form
        @if (!string.IsNullOrEmpty(formQuotationData.Ref))
        {
            <span style="color: #448dba; font-weight: bold">(REF # @formQuotationData.Ref)</span>
        }

        @if (IsNewProductMaster)
        {
            <a class="btn btn-primary" style="float:right" href="/Form/ProductMaster">Back</a>
        }
        else
        {
            <a class="btn btn-primary" style="float:right" href="/Form/ScientistNewForm">Cancel</a>
            <a class="btn btn-primary" style="float:right;margin-right:7px;" href="/Form/ViewForm/@quotationDetailsId">Back to List</a>
        }
    </h1>
</section>
<section class="content">
    <div class="row">
        <!-- left column -->
        <div class="col-md-12">
            <!-- general form elements -->
            <div class="box box-primary">
                @if (formQuotationData.ApprovalStatus == Convert.ToString((int)Synzeal_Inventory.Models.EnumList.ApprovedStatus.QCCorrection))
                {
                    <div style="background: #f57575; color: #fff; padding: 6px 20px; font-size: 17px; font-weight: bold;">QC Team submitted this form with correction. Please review comments below and resolved it.</div>
                }
                @if (formQuotationData.ApprovalStatus == Convert.ToString((int)Synzeal_Inventory.Models.EnumList.ApprovedStatus.QCApproved))
                {
                    <div style="background: #71d271; color: #000; padding: 6px 20px; font-size: 17px; font-weight: bold;">QC Team approved this form.</div>
                }
                <!-- /.box-header -->
                <!-- form start -->
                <form role="form" id="makeform" enctype="multipart/form-data">
                    <div class="box-body">
                        <div class="col-md-12" style="font-size: 18px; margin-bottom: 10px;">
                            <b>
                                Submission Details
                            </b>
                        </div>
                        <div class="form-group col-md-3">
                            <input type="hidden" id="IsNewProductMaster" name="IsNewProductMaster" value="@IsNewProductMaster" />
                            <input type="hidden" id="QuotationDetailsId" name="QuotationDetailsId" value="@ViewBag.QuotationDetailsId" />
                            <input type="hidden" id="QuotationDetailsFormId" name="QuotationDetailsFormId" value="@ViewBag.FormId" />
                            <label for="exampleInputEmail1">Submission Date :</label>
                            <input type="text" class="form-control" id="SubmissionDate" name="SubmissionDate" placeholder="Submission Date" required>
                        </div>
                        <div class="form-group col-md-3">
                            <label for="exampleInputEmail1">Journal Date :</label>
                            <input type="text" class="form-control" id="JournalDate" name="JournalDate" placeholder="Journal Date" required>
                        </div>
                        <div class="form-group col-md-3">
                            <label for="exampleInputPassword1">TL Name:</label>
                            <input type="email" class="form-control" id="TLName" name="TLName" placeholder="TL Name" value="@Synzeal_Inventory.Models.SessionCookieManagement.UserName">
                        </div>
                        <div class="form-group col-md-3">
                            <label for="exampleInputPassword1">Project Name:</label>
                            <input type="text" class="form-control" id="ProjectName" name="ProjectName" placeholder="Project Name" required>
                        </div>
                        <div class="form-group col-md-3">
                            <label for="exampleInputPassword1">Sci. Name:</label>
                            <input type="email" class="form-control" id="NameOfScientist" name="NameOfScientist" placeholder="Name Of Scientist" value="@Model.SubScientistName">
                        </div>
                        <div class="form-group col-md-3">
                            <label for="exampleInputPassword1">Quantity Submitted (in mg):<sup style="color:red">(*)</sup></label>
                            <input type="text" class="form-control" id="QuantitySubmitted" name="QuantitySubmitted" placeholder="Quantity Submitted">
                        </div>
                        <div class="form-group col-md-3">
                            <label for="exampleInputPassword1">
