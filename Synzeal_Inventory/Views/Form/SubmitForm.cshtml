<script>
    $(function () {
        CKEDITOR.replace('AnalysisCode');
    });
    $("#Isboughtout").change(function () {
        var value = $(this).val();
        if (value === 'true') {
            $("#divboughtout").show();
        }
        else {
            $("#divboughtout").false();
        }
    });
    function approCommentGenerate(id) {
        $.ajax({
            url: '/Form/AddApprovComment?id=' + id + '&isScientist=true',
            data: {},
            type: 'GET',
            traditional: true, // add this
            dataType: 'json',
            success: function (data) {
                $('#invoiceQueryModal .modal-body').html(data.html);
                $('#invoiceQueryModal').modal({ show: true });
                // $("#invoiceQueryModal .modal-content").css("width", "1080px");
            }
        });
    }

$(function() {
