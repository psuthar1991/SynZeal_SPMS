                    <button type="button" onclick="Quote.ChangeAvailableDataforForm()" class="btn btn-primary">Save</button>
                </div>
            </div>
        </form>
    </div>
</div>

@if (!string.IsNullOrEmpty(Model.RbAdditionalAnalysis))
{
    <text>
        <script type="text/javascript">
            var splirec = '@Model.RbAdditionalAnalysis'.split(',');
                $(splirec).each(function(i, v) {
                    $("input[name=radioAdditionalAnalysis][value='" + v + "']").prop("checked", true);
                });
        </script>
    </text>
}