        function websiteDeployNotificationUserside() {
            $.ajax({
                url: '/Form/WebsiteDeployNotification',
                data: {},
                type: 'GET',
                async: true,
                success: function (data) {
                    if (data.isDeploy === "true") {
                        $("#alertnotification").fadeIn();

                        var dateString = data.date.substr(6);
                        var currentTime = new Date(parseInt(dateString));
                        var month = currentTime.getMonth() + 1;
                        var day = currentTime.getDate();
                        var year = currentTime.getFullYear();
                        var date = day + "/" + month + "/" + year + " " + currentTime.getHours() + ":" + currentTime.getMinutes();
                        $(".main-header").css("margin-top", "35px");
                        $("#alertnotification").text("We're going to deploy some new features. Please save all your progress within 10 min. ");
                    }
                    else {
                        $("#alertnotification").fadeOut();
                    }
                }
            });
        }

        setTimeout(function () {
