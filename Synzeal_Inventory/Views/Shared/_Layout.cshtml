            $(this).addClass('active');
            var userID = $(this).attr('data-user-id');
            initiateChat(userID);
        });
        function searchFriends() {
            var searchText = $('#txtSearch').val();
            if (searchText != null && searchText != '') {
                $("#divBody").html('');
                $("#divBody").load('/User/_UserSearchResult?name=' + searchText + '', function () {

                });
