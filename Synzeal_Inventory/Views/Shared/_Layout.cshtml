        function OpenResQty() {
            var pathname = window.location.pathname;
            if (window.location.pathname.toLowerCase().indexOf("quotationlist") === -1 &&
                window.location.pathname.toLowerCase().indexOf("project") === -1) {
                toastr.error("This feature is only valid for quotation list and project tab.");
                return false;
            }
            var refData = '';
            var ids = [];
            var menulink = '';
            $('.nav').find('li').each(function () {
                if ($(this).hasClass('active')) {
                    menulink = $(this).find('a').attr('data-tabname');
                }
            });
            var tableId = "";
            if (window.location.pathname.toLowerCase().indexOf("quotationlist") !== -1) {
                refData = 'quote';
                if (menulink === 'all') {
                    tableId = 'example1';
                }
                else if (menulink === 'tobe') {
                    tableId = 'tbltobe';
                }
                else if (menulink === 'tobedistinutor') {
                    tableId = 'tbltobedistributor';
                }
                else if (menulink === 'followup') {
                    tableId = 'tblfollowup';
                }
                else if (menulink === 'auction') {
                    tableId = 'tblauction';
                }
                else {
                    tableId = 'tblinhouse';
                }

                $("#" + tableId).find(".clsclubQuote").each(function () {
                    if ($(this).is(':checked')) {
                        ids.push($(this).val());
                    }
                });
            }
            if (window.location.pathname.toLowerCase().indexOf("project") !== -1) {
                refData = 'project';
                if (menulink === 'all') {
                    tableId = 'example1';
                }
                else if (menulink === 'noaction') {
                    tableId = 'tblnoaction';
                }
                else if (menulink === 'domestic') {
                    tableId = 'tbldomestic';
                }
                else if (menulink === 'synthesis') {
                    tableId = 'tblsynthesis';
                }
                else if (menulink === 'purchase') {
                    tableId = 'tblpurchase';
                }
                else if (menulink === 'instock') {
                    tableId = 'tblinstock';
                }
                else if (menulink === 'export') {
                    tableId = 'tblexport';
                }
                else if (menulink === 'controlledsubstance') {
                    tableId = 'tblcontrolledsubstance';
                }
                else if (menulink === 'hold') {
                    tableId = 'example2';
                }
                else {
                    tableId = 'exampleinhouse';
                }
                $("#" + tableId).find(".clsSaverow").each(function () {
                    if ($(this).is(':checked')) {
                        ids.push($(this).val());
                    }
                });
            }

            if (ids.length > 1) {
                toastr.error("Please select only single product.");
                return false;
            }

            $.ajax({
                url: '/Form/AddReservedQty?id=' + ids[0] + "&refData=" + refData,
                data: {},
                type: 'GET',
                traditional: true,
                dataType: 'json',
                success: function (data) {
                    $('#invoiceQueryModal .modal-body').html(data);
                    $('#invoiceQueryModal').modal({ show: true });
                }
            });
        }
    </script>
